#!name=局域网服务主机调整
#!desc=alist、web2api、肥羊在shzlk和H&B_Family及其他网络环境中运行，选择不同服务主机

[Rule]
DOMAIN,mac-mini.local,DIRECT
DOMAIN,xueiMac.local,DIRECT

[Script]
feiyang = type=http-request,pattern=^https:\/\/feiyang\.allinhub\.top\/.*,requires-body=1,script-path=https://raw.githubusercontent.com/abingx/Self-Rule/master/Surge/script/feiyang.js
localapi = type=http-request,pattern=^https:\/\/(qwen|metaso|kimi)\.allinhub\.top\/.*,requires-body=1,script-path=https://raw.githubusercontent.com/abingx/Self-Rule/master/Surge/script/localapi.js

[MITM]
hostname = %APPEND% feiyang.allinhub.top, qwen.allinhub.top, kimi.allinhub.top, metaso.allinhub.top